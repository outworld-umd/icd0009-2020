@model WebApp.Areas.Restaurant.ViewModels.ItemTypeCreateEditViewModel

@{
    ViewData["Title"] = @Resources.Views.CreDelDetEdiInd.CreDelDetEdiInd.Edit;
}

<h1>@Resources.Views.CreDelDetEdiInd.CreDelDetEdiInd.Edit</h1>

<h4>@Resources.Views.CreDelDetEdiInd.CreDelDetEdiInd.ItemType</h4>
<hr/>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <partial name="Shared/_CreateEdit"/>
            <input type="hidden" asp-for="ItemType.Id"/>
            <div class="form-group">
                <input type="submit" value="@Resources.Views.CreDelDetEdiInd.CreDelDetEdiInd.Save" class="btn btn-primary"/>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@Resources.Views.CreDelDetEdiInd.CreDelDetEdiInd.Back</a>
</div>

@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}